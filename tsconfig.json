{
  "compilerOptions": {
    "types": ["vite/client"],
    "target": "ES2022",
    "jsx": "react-jsx",
    "rootDir": "./src",
    "outDir": "/js",
    "module": "ES2022",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}